<%- include('./nav.ejs') %>
<body>
	<div class="container pt-4 pb-5">
		<h4 class="text-muted" style="display: inline">User</h4>
		<button class="btn btn-link btn-floating" type="button">
			<i class="fa-solid fa-plus"></i>
		</button>
		<table class="table">
			<thead>
				<tr>
					<th scope="col">ID</th>
					<th scope="col">Name </th>
					<th scope="col">Role</th>
					<th scope="col">Action</th>
				</tr>
			</thead>
			<% var weekdays = ["Sunday", "Monday", "Tuesday", "Wednesday",
			"Thursday", "Friday", "Saturday"] %>
			<tbody>
				<% data.users.forEach((user)=> { %>
				<tr>
					<td>
						<input
							type="text"
							class="form-control"
							value="<%= user.id%>"
							
						/>
					</td>
					<td>
						<input
							type="text"
							class="form-control"
							value="<%= user.name%>"
							
						/>
					</td>
					<td>
						<input
							type="text"
							class="form-control"
							value="<%=user.role%>"
							
						/>
					</td>
					<td>
						<button
							class="btn btn-light btn-rounded btn-sm fw-bold"
							data-toggle="modal"
							data-target="#exampleModal3"
							type="button"
							data-mdb-ripple-color="dark"
							onclick=""
						>
							Edit
						</button>
						<button
							class="btn btn-danger btn-rounded btn-sm fw-bold"
							data-toggle="modal"
							data-target="#exampleModal4"
							type="button"
							data-mdb-ripple-color="dark"
							onclick=""
						>
							Delete
						</button>
					</td>
				</tr>
				<% }) %>
			</tbody>
		</table>
		<div class="float-center d-flex justify-content-center">
			<button class="btn btn-link">back</button>
		</div>
	</div>
</body>
<%- include('./nav_end.ejs') %>
