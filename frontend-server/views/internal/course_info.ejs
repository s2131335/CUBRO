<%- include('./nav.ejs') %>
<body>
	<div class="container pt-4 pb-5">
		<div class="row">
			<div class="col-10">
				<h3 style="display: inline"><%=courseCode%>-<%= classNo %></h3>
				<h3 style="display: inline" class="ps-3 text-muted">
					<%=courseName%>
				</h3>
			</div>
			<div class="col-2">
				<button class="btn btn-primary float-end">Select</button>
			</div>
		</div>
		<hr class="my-2" />
		<p class="text-muted"><%=description%></p>
		<table class="table">
			<thead>
				<tr>
					<th scope="col">Instructor</th>
					<th scope="col">Seats</th>
					<th scope="col">Venue</th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<td><%=instructor%></td>
					<td><%=seat%></td>
					<td><%=venue%></td>
				</tr>
			</tbody>
		</table>
		<h4 class="text-muted">Meetings</h4>
		<table class="table">
			<thead>
				<tr>
					<th scope="col">Weekday</th>
					<th scope="col">Time</th>
					<th scope="col">Dates</th>
				</tr>
			</thead>
			<% var weekdays = ["Sunday", "Monday", "Tuesday", "Wednesday",
			"Thursday", "Friday", "Saturday"] %>
			<tbody>
				<% for (let meeting of meetings) { %>
				<tr>
					<td><%= weekdays[meeting.day]%></td>
					<td><%=meeting.start%> - <%=meeting.end%></td>
					<td><%=meeting.dates%></td>
				</tr>
				<%}%>
			</tbody>
		</table>
		<div class="float-center d-flex justify-content-center">
			<button class="btn btn-link">back</button>
		</div>
	</div>
</body>
<%- include('./nav_end.ejs') %>
